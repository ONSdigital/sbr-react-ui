{
  "name": "sbr-ui",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "repository": "https://github.com/ONSdigital/sbr-ui.git",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.0.2",
    "mz": "^2.4.0",
    "react-scripts": "0.2.1",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.0"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "coveralls": "^2.13.1",
    "express": "^4.14.0",
    "istanbul": "^0.4.5",
    "jasmine": "^2.5.3",
    "jasmine-node": "^1.14.5",
    "morgan": "^1.7.0",
    "npm-run-script": "0.0.4",
    "object.assign": "^4.0.4",
    "rc-pagination": "^1.7.3",
    "react": "^15.5.4",
    "react-bootstrap": "^0.30.7",
    "react-bootstrap-button-loader": "^1.0.8",
    "react-bootstrap-table": "^3.1.1",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^2.6.1",
    "react-router-bootstrap": "^0.23.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "selenium-webdriver": "^3.3.0"
  },
  "engines": {
    "node": "5.10.1",
    "npm": "3.8.3"
  },
  "scripts": {
    "start": "npm run build; SERVE_HTML=true ENV=local node server",
    "restart": "react-scripts start",
    "start:server": "node server",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "SERVE_HTML=true mocha test/server.test.js; NODE_ENV=test jasmine test/integration-test.js",
    "cover": "NODE_ENV=test SERVE_HTML=true ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec test/server.test.js && ./node_modules/istanbul/lib/cli.js cover jasmine-node -- --verbose --captureExceptions test/integration-test.js",
    "cover-jasmine": "UI_URL=http://localhost:3000 NODE_ENV=test ./node_modules/istanbul/lib/cli.js cover ./node_modules/.bin/jasmine --verbose --captureExceptions test/integration-test.js",
    "cover-test": "UI_URL=http://localhost:3000 NODE_ENV=test istanbul cover jasmine test/integration-test.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js",
    "env": {
      "mocha": true
    },
    "rules": {
      "strict": 0
    }
  }
}
